# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure(2) do |config|

  config.vm.box = "http://cloud-images.ubuntu.com/snappy/15.04/core/stable/current/core-stable-amd64-vagrant.box"
  config.vm.provider "virtualbox" do |v|
        v.memory = 512
        v.cpus = 2
        v.gui = true
    end
end
